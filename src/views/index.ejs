<nav aria-label="breadcrumb justify-content-center">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active"><a href="#">Home</a></li>
    </ol>
</nav>
<div class="container-fluid" style="padding-bottom: 20px;">
    <% data.forEach(function(item) { %>
        <div class="wrapper" style="margin-bottom: 20px; justify-content: center;">
            <div class=" bootstrap-card">
                <div class="card-body">
                    <img class="card-img-top" src="/images/<%= item.image %>" alt="Card image cap">
                    <h5 class="card-title">
                        <%= item.name %>
                    </h5>
                    <p class="card-text">
                        <%= item.page_description %>
                    </p>
                    <a href="/category/<%= item.id %>" class="btn btn-success">View Products</a>
                </div>
            </div>
        </div>
        <% }); %>


</div>